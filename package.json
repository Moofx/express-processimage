    "optimist": "0.6.1",
    "optipng": "0.3.1",
    "passerror": "1.1.1",
    "pngcrush": "0.2.0",
    "pngquant": "0.6.0",
    "sharp": "0.13.1",
    "underscore": "1.8.3"
  },
  "optionalDependencies": {
    "svgfilter": "1.1.0"
