{
  "name": "express-processimage",
  "version": "2.1.0",
  "description": "Express middleware that processes served images according to the query string",
  "main": "lib/processImage.js",
  "directories": {
    "test": "test",
    "bin": "bin"
  },
  "dependencies": {
    "createerror": "0.5.1",
    "express-hijackresponse": "0.2.1",
    "gm": "1.17.0",
    "inkscape": "0.0.5",
    "jpegtran": "0.0.6",
    "mime": "1.2.11",
    "optimist": "0.6.1",
    "optipng": "0.1.1",
    "passerror": "0.0.1",
    "pngcrush": "0.1.0",
    "pngquant": "0.3.0",
    "underscore": "1.8.3"
  },
  "optionalDependencies": {
    "sharp": "0.9.0",
    "svgfilter": "1.0.0"
  },
  "devDependencies": {
    "coveralls": "2.11.2",
    "express": "4.12.3",
    "istanbul": "0.3.13",
    "jshint": "2.7.0",
    "messy": "6.2.0",
    "mocha": "2.2.4",
    "unexpected": "7.0.5",
    "unexpected-express": "6.1.0",
    "unexpected-image": "1.0.0",
    "unexpected-messy": "3.0.3"
  },
  "scripts": {
